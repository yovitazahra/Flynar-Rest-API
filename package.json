{
  "name": "c5-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.ts",
  "scripts": {
    "dev": "nodemon ./app/index.ts",
    "start": "ts-node ./app/index.ts",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:init": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:remake": "npm run db:drop && npm run db:init",
    "db:migrate:users": "sequelize db:migrate --to 20230606132817-create-users.js",
    "db:rollback:users": "sequelize db:migrate:undo:all --to 20230606132817-create-users.js",
    "db:recreate:users": "npm run db:rollback:users && npm run db:migrate:users",
    "db:seed:users": "sequelize db:seed --seed 20230606141740-insert-users.js",
    "db:migrate:flights": "sequelize db:migrate --to 20230610125723-create-flights.js",
    "db:rollback:flights": "sequelize db:migrate:undo:all --to 20230610125723-create-flights.js",
    "db:recreate:flights": "npm run db:rollback:flights && npm run db:migrate:flights",
    "db:seed:flights": "sequelize db:seed --seed 20230607124144-insert-flights.js",
    "db:migrate:tickets": "sequelize db:migrate --to 20230620115940-create-tickets.js",
    "db:rollback:tickets": "sequelize db:migrate:undo:all --to 20230620115940-create-tickets.js",
    "db:recreate:tickets": "npm run db:rollback:tickets && npm run db:migrate:tickets",
    "db:seed:tickets": "sequelize db:seed --seed 20230620120010-insert-tickets.js",
    "db:migrate:checkouts": "sequelize db:migrate --to 20230621111914-create-checkouts",
    "db:rollback:checkouts": "sequelize db:migrate:undo:all --to 20230621111914-create-checkouts",
    "db:recreate:checkouts": "npm run db:rollback:checkouts && npm run db:migrate:checkouts",
    "lint": "eslint ./",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mailgen": "^2.0.27",
    "nodemailer": "^6.9.3",
    "otp-generator": "^4.0.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "randomstring": "^1.3.0",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^8.38.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "18.x.x"
  }
}
